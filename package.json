{
  "name": "action-binder",
  "version": "0.0.1",
  "author": "lavsiant",
  "main": "electron/dist/main.js",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "electron": "ng build --base-href ./ && tsc --p electron && electron electron/dist/main.js",
    "electron-dev": "concurrently \"ng serve\" \"wait-on http://localhost:4200 &&  electron electron/dist/main.js .\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/cdk": "^15.1.2",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/material": "^15.1.2",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@electron/remote": "^2.0.9",
    "electron-json-storage": "^4.6.0",
    "rxjs": "~7.8.0",
    "source-map-support": "^0.5.16",
    "text-from-image": "^1.1.1",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "wait-on": "^7.0.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.3",
    "@angular/cli": "~15.1.3",
    "@angular/compiler-cli": "^15.1.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "concurrently": "^7.6.0",
    "electron": "^22.1.0",
    "electron-builder": "^23.6.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
